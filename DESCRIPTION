Package: tfruns
Type: Package
Title: Training Run Tools for 'TensorFlow'
Version: 0.9.1.9000
Authors@R: c(
  person("JJ", "Allaire", role = c("aut", "cre"), email = "jj@rstudio.com"),
  person(family = "RStudio", role = c("cph", "fnd")),
